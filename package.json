{
  "name": "next-tinynewsdemo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "ORG_SLUG=oaklyn-test NEXT_PUBLIC_CYPRESS_TESTING=true CYPRESS_API_TOKEN=$API_TOKEN HASURA_API_URL=https://tinynews-testing.hasura.app/v1/graphql next dev",
    "seed": "ORG_SLUG=oaklyn-test NEXT_PUBLIC_CYPRESS_TESTING=true CYPRESS_API_TOKEN=$API_TOKEN HASURA_API_URL=https://tinynews-testing.hasura.app/v1/graphql node script/seed.js",
    "build:test": "ORG_SLUG=oaklyn-test NEXT_PUBLIC_CYPRESS_TESTING=true CYPRESS_API_TOKEN=$API_TOKEN HASURA_API_URL=https://tinynews-testing.hasura.app/v1/graphql next build",
    "clear": "node script/clear.js",
    "populate": "node script/populate.js",
    "wordpress": "node script/wordpress.js",
    "data:clear": "node script/clear-analytics.js",
    "bootstrap": "node script/bootstrap.js",
    "bootstrap:tests": "node script/bootstrap-tests.js",
    "status": "node script/status.js",
    "remove:org": "node script/remove.js",
    "test": "jest --runInBand",
    "build": "NODE_OPTIONS='--trace-warnings' next build",
    "cy:run": "cypress run",
    "cypress:open": "NEXT_PUBLIC_CYPRESS_TESTING=true cypress open",
    "cypress:api": "npm run cy:run -- --env CYPRESS_API_TOKEN=$CYPRESS_API_TOKEN --spec 'cypress/integration/documents_api_lookup_spec.js'",
    "build:rss": "node script/generate-rss.js",
    "build:newsletters": "node script/build-newsletter-editions.js",
    "prebuild": "node script/build-newsletter-editions.js",
    "postbuild": "next-sitemap && node script/generate-rss.js",
    "start": "next start",
    "export": "next build && next export",
    "purgecss": "purgecss --css styles/bulma.css --content pages/** components/** --output ./purge.css",
    "lint": "eslint ./components/** ./pages/** ./lib/** -c .eslintrc.json --fix --ext js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ./components/** ./pages/** ./lib/**"
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "lint-staged": {
    "./components/**": "npm run format",
    "./pages/**": "npm run format",
    "./lib/**": "npm run format",
    "./styles/**": "npm run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "testTimeout": 30000,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@babel/runtime": "^7.13.10",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@heroicons/react": "^1.0.1",
    "@mailchimp/mailchimp_marketing": "^3.0.36",
    "@octokit/rest": "^18.5.3",
    "@tailwindcss/forms": "^0.3.2",
    "@tinymce/tinymce-react": "^3.12.6",
    "aws-sdk": "^2.1046.0",
    "bulma": "^0.9.0",
    "chart.js": "^2.9.4",
    "commander": "^7.1.0",
    "cookie": "^0.3.1",
    "date-fns": "^2.14.0",
    "date-fns-tz": "^1.1.6",
    "fetch-cookie": "^0.10.1",
    "formik": "^2.1.4",
    "googleapis": "^67.1.1",
    "graphql-request": "^2.0.0",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.1",
    "image-type": "^4.1.0",
    "journalize": "^2.4.0",
    "js-yaml": "^4.1.0",
    "local-storage-fallback": "^4.1.1",
    "markdown-to-jsx": "^7.1.3",
    "next": "^11.1.3",
    "next-auth": "^3.6.1",
    "node-fetch": "^2.6.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "purgecss": "^2.3.0",
    "react": "^17.0.2",
    "react-analytics-charts": "^1.2.12",
    "react-aws-s3": "^1.5.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-chartjs-2": "^2.11.1",
    "react-cookie": "^4.0.3",
    "react-cookie-consent": "^6.4.1",
    "react-currency-input-field": "^3.6.0",
    "react-datepicker": "^3.6.0",
    "react-dates": "^21.8.0",
    "react-dom": "^17.0.2",
    "react-facebook": "^8.1.4",
    "react-ga": "^3.1.2",
    "react-google-analytics": "^0.2.0",
    "react-google-oauth": "^1.0.0",
    "react-instagram-embed": "^2.0.1",
    "react-intersection-observer": "^8.27.1",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-player": "^2.5.0",
    "react-scroll-percentage": "^4.2.0",
    "react-tiktok": "^1.0.0",
    "react-twitter-embed": "^3.0.3",
    "react-with-direction": "^1.3.1",
    "recharts": "^2.0.9",
    "remark": "^14.0.1",
    "remark-html": "^15.0.0",
    "rss": "^1.2.2",
    "sass": "^1.26.9",
    "styled-component": "^2.8.0",
    "styled-components": "^5.2.3",
    "swr": "^0.5.6",
    "tweetsodium": "^0.0.5",
    "universal-cookie": "^4.0.4",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.3",
    "wxr-generator": "^0.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-styled-components": "^1.13.3",
    "cypress": "^9.3.1",
    "cypress-dotenv": "^2.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.4.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "faker": "^5.5.3",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.11",
    "next-sitemap": "^1.5.10",
    "postcss": "^8.2.10",
    "prettier": "2.0.5",
    "react-is": "^17.0.2",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.1.1",
    "twin.macro": "^2.3.2"
  }
}
