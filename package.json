{
  "name": "next-tinynewsdemo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clear": "node script/clear.js",
    "populate": "node script/populate.js",
    "bootstrap": "node script/bootstrap.js",
    "build": "next build",
    "build:rss": "node script/generate-rss.js",
    "postbuild": "next-sitemap && node script/generate-rss.js",
    "start": "next start",
    "export": "next build && next export",
    "purgecss": "purgecss --css styles/bulma.css --content pages/** components/** --output ./purge.css",
    "lint": "eslint ./components/** ./pages/** ./lib/** -c .eslintrc.json --fix --ext js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ./components/** ./pages/** ./lib/**"
  },
  "lint-staged": {
    "./components/**": "npm run format",
    "./pages/**": "npm run format",
    "./lib/**": "npm run format",
    "./styles/**": "npm run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@heroicons/react": "^1.0.1",
    "@mailchimp/mailchimp_marketing": "^3.0.36",
    "bulma": "^0.9.0",
    "chart.js": "^2.9.4",
    "commander": "^7.1.0",
    "cookie": "^0.3.1",
    "date-fns": "^2.14.0",
    "dotenv": "^8.2.0",
    "fetch-cookie": "^0.10.1",
    "formik": "^2.1.4",
    "googleapis": "^67.1.1",
    "graphql-request": "^2.0.0",
    "journalize": "^2.4.0",
    "local-storage-fallback": "^4.1.1",
    "moment": "^2.29.1",
    "next": "^10.0.1",
    "next-auth": "^3.6.1",
    "node-fetch": "^2.6.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "purgecss": "^2.3.0",
    "react": "^16.14.0",
    "react-analytics-charts": "^1.2.12",
    "react-aws-s3": "https://github.com/news-catalyst/react-aws-s3.git",
    "react-chartjs-2": "^2.11.1",
    "react-cookie": "^4.0.3",
    "react-datepicker": "^3.6.0",
    "react-dates": "^21.8.0",
    "react-dom": "^16.14.0",
    "react-ga": "^3.1.2",
    "react-google-analytics": "^0.2.0",
    "react-google-oauth": "^1.0.0",
    "react-intersection-observer": "^8.27.1",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-player": "^2.5.0",
    "react-scroll-percentage": "^4.2.0",
    "react-twitter-embed": "^3.0.3",
    "react-with-direction": "^1.3.1",
    "rss": "^1.2.2",
    "sass": "^1.26.9",
    "styled-component": "^2.8.0",
    "styled-components": "^5.2.3",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^0.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "next-sitemap": "^1.5.10",
    "postcss": "^8.2.10",
    "prettier": "2.0.5",
    "react-is": "^17.0.2",
    "tailwindcss": "^2.1.1",
    "twin.macro": "^2.3.2"
  }
}
