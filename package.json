{
  "name": "next-tinynewsdemo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./components/** ./pages/** ./lib/** -c .eslintrc.json --fix --ext js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ./components/** ./pages/** ./lib/**"
  },
  "lint-staged": {
    "./components/**": "npm run lint",
    "./pages/**": "npm run lint",
    "./lib/**": "npm run lint",
    "./components/**": "npm run format",
    "./pages/**": "npm run format",
    "./lib/**": "npm run format",
    "./styles/**": "npm run format"
  },
  "husky": {
     "hooks": {
       "pre-commit": "lint-staged"
     }
  },
  "dependencies": {
    "@next/plugin-google-analytics": "^9.4.4",
    "bulma": "^0.9.0",
    "date-fns": "^2.14.0",
    "formik": "^2.1.4",
    "graphql-request": "^2.0.0",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "^3.1.2",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-player": "^2.5.0",
    "react-twitter-embed": "^3.0.3",
    "web-vitals": "^0.2.3"
    "sass": "^1.26.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5"
  }
}
